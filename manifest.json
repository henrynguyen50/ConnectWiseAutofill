{
    "name": "ConnectWise Company Autofill",
    "description": "Autofill company information to prevent mismatches",
    "version": "2.0",
    "manifest_version": 3,
    "permissions": [
        "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "ConnectWise Icon.png"
    },
    
    "content_scripts": [
      {
        "js": ["scripts/content.js"],
        "matches": [
            "https://na.myconnectwise.net/*"
        ]
      }  
    ]
    

  }